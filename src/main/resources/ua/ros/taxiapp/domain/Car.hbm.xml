<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="ua.ros.taxiapp.domain">
    <class name="Car" table="cars">
        <id name = "carId" type="int" column = "ID_CAR">
            <generator class = "native"/>
        </id>
        <many-to-one name="brand" class="Brand">
            <column name = "ID_BRAND" not-null="true"/>
        </many-to-one>
        <many-to-one name="model" class="Model">
            <column name = "ID_MODEL" not-null="true"/>
        </many-to-one>
        <property name = "year" column = "YEAR" type = "integer"/>
        <property name = "regNumber" column = "REG_NUMBER" type = "string"/>
        <property name = "pricePerKm" column = "PRICE_PER_KM" type = "double"/>   
    </class> 
</hibernate-mapping>