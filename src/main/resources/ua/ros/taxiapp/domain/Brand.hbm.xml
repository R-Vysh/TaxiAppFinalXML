<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="ua.ros.taxiapp.domain.Brand" table="brands">
        <id name = "brandId" type="int" column = "ID_BRAND">
            <generator class = "native"/>
        </id>
        <property name = "brandsName" column = "NAME" type = "string"/>
        <bag name="models" table="models" 
				inverse="true" lazy="true" fetch="select">
            <key>
                <column name="BRAND_ID" not-null="true" />
            </key>
            <one-to-many class="ua.ros.taxiapp.domain.Model" />
        </bag>
    </class> 
</hibernate-mapping>