<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="ua.ros.taxiapp.domain">
    <class name="Model" table="models">
        <id name = "modelId" type="int" column = "ID_MODEL">
            <generator class = "native"/>
        </id>
        <property name = "modelsName" column = "NAME" type = "string"/>
        <many-to-one name="brand" class="Brand">
            <column name = "ID_BRAND" not-null="true"/>
        </many-to-one>
    </class> 
</hibernate-mapping>